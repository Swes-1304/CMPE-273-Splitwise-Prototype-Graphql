{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": ["error"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": [0,{ "ignore": "ignore", "customValidators": "customValidator" }]
      },
      "settings": {
        "react" : {
            "pragma": "React",
            "version": "16.8"
        }
    }
  }